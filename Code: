#include <iostream>
using namespace std;

int mySqrt(int x) {
    int i = 0;

    while (i * i <= x) {
        i ++;
    }

    i--; 
    return i;
}

int main() {
    int x;
    cout << "Enter a number: ";
    cin >> x;

    int result = mySqrt(x);
    cout << "The rounded down sqrt is: " << result << endl;

    return 0;
}
